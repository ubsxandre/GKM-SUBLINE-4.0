      "application/json"
    ],
    "produces": [
      "application/json"
    ],
    "requestBody":{
      "content": {
        "application/json":{
          "schema": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "id_mesin": {
                "type": "integer"
              },
              "model_id": {
                "type": "string"
              },
              "nama_produk": {
                "type": "string",
                "example": "cth: C,R,U,D"
              },
              "rpm": {
                "type": "integer"
              }
            }
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "User granted access"
      },
      "400": {
        "description": "Invalid request data"
      }
    }
  },
