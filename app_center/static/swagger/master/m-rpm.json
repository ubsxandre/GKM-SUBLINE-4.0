{
  "get": {
    "summary": "Get a list of Master RPM",
    "description": "Returns a list of rpm",
    "tags": ["m-rpm"],
    "parameters": [
      {
        "in": "query",  
        "name": "id",
        "description": "id rpm",
        "type": "int",
        "required": false
      },
      {
        "in": "query",  
        "name": "id_mesin",
        "description": "id mesin",
        "type": "int",
        "required": false
      },
      {
        "in": "query",  
        "name": "model_id",
        "description": "model id",
        "type": "string",
        "required": false
      },
      {
        "in": "query",  
        "name": "nama_produk",
        "description": "nama produk",
        "type": "string",
        "required": false
      },
      {
        "in": "query",  
        "name": "rpm",
        "description": "rpm",
        "type": "integer",
        "required": false
      }
    ],
    "responses": {
      "200": {
        "description": "User granted access"
      },
      "400": {
        "description": "Invalid request data"
      },
      "404": {
        "description": "Data Not Found"
      }
    }
  },
